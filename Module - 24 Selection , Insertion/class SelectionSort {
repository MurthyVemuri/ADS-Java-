class SelectionSort {
	public void selectionSort(int[] list){
		for (int i=0;i<list.length;i++){
		    for (int si=i;si<list.length;si++){
		        if (list[si]<list[i]){
		            int temp=list[i];
		            list[i]=list[si];
		            list[si]=temp;
		        }
		    }
		}        
	}
}

public class SelectionDemo {
	
}